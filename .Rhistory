# ğŸ¯ Step 1: Global average rating
mu <- mean(edx$rating)
# ğŸ” Step 2: Try multiple lambda values
lambdas <- seq(0, 10, 0.25)
# ğŸ“‰ Step 3: Define RMSE function
RMSE <- function(true_ratings, predicted_ratings) {
sqrt(mean((true_ratings - predicted_ratings)^2))
}
# ğŸ§ª Step 4: Loop over lambdas and compute RMSE
rmse_results <- sapply(lambdas, function(lambda) {
# Regularized movie effects
b_i <- edx %>%
group_by(movieId) %>%
summarize(b_i = sum(rating - mu) / (n() + lambda))
# Regularized user effects
b_u <- edx %>%
left_join(b_i, by = "movieId") %>%
group_by(userId) %>%
summarize(b_u = sum(rating - mu - b_i) / (n() + lambda))
# Predict on final_holdout_test
predicted_ratings <- final_holdout_test %>%
left_join(b_i, by = "movieId") %>%
left_join(b_u, by = "userId") %>%
mutate(pred = mu + b_i + b_u) %>%
pull(pred)
# Calculate RMSE
RMSE(final_holdout_test$rating, predicted_ratings)
})
